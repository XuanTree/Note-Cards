{
  "name": "note-card-view",
  "displayName": "Note Cards",
  "description": "A simple extension to let you view all your md notes as cards!",
  "version": "0.0.3",
  "publisher": "XuanTree",
  "repository": {
    "type": "git",
    "url": "https://github.com/XuanTree/Note-Cards.git"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "note-base-view.openWebView",
        "title": "Note Cards: Open View"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.11",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vscode": "^1.1.37"
  }
}
